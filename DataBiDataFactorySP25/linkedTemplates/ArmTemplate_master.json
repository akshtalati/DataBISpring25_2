{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "DataBiDataFactorySP25"
		},
		"AzureDataLakeStorage1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage1'"
		},
		"AzureDataLakeStorage2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage2'"
		},
		"Snowflake1_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'Snowflake1'"
		},
		"Snowflake2_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'Snowflake2'"
		},
		"Snowflake3_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Snowflake3'"
		},
		"SnowflakeLATEARRIVAL_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SnowflakeLATEARRIVAL'"
		},
		"AzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://datalakespring25.dfs.core.windows.net/"
		},
		"AzureDataLakeStorage2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://datalakespring25.dfs.core.windows.net/"
		},
		"AzureKeyVault1_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://databispringkeyvaultsp25.vault.azure.net/"
		},
		"Blob_sasUri": {
			"type": "secureString",
			"metadata": "Secure string for 'sasUri' of 'Blob'"
		},
		"Snowflake1_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "SEATTLE_PET_LIC_DB"
		},
		"Snowflake1_properties_typeProperties_role": {
			"type": "string",
			"defaultValue": "SEATTLE_PET_LIC_ROLE"
		},
		"Snowflake1_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "PGRECHG-QGA23502.snowflakecomputing.com"
		},
		"Snowflake2_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "SEATTLE_PET_LIC_DB"
		},
		"Snowflake2_properties_typeProperties_role": {
			"type": "string",
			"defaultValue": "SEATTLE_PET_LIC_ROLE"
		},
		"Snowflake2_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "PGRECHG-QGA23502.snowflakecomputing.com"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "DataBiDataFactorySP25_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorage1_accountKey": {
						"value": "[parameters('AzureDataLakeStorage1_accountKey')]"
					},
					"AzureDataLakeStorage2_accountKey": {
						"value": "[parameters('AzureDataLakeStorage2_accountKey')]"
					},
					"Snowflake1_password": {
						"value": "[parameters('Snowflake1_password')]"
					},
					"Snowflake2_password": {
						"value": "[parameters('Snowflake2_password')]"
					},
					"Snowflake3_connectionString": {
						"value": "[parameters('Snowflake3_connectionString')]"
					},
					"SnowflakeLATEARRIVAL_connectionString": {
						"value": "[parameters('SnowflakeLATEARRIVAL_connectionString')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_url')]"
					},
					"AzureDataLakeStorage2_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage2_properties_typeProperties_url')]"
					},
					"AzureKeyVault1_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVault1_properties_typeProperties_baseUrl')]"
					},
					"Blob_sasUri": {
						"value": "[parameters('Blob_sasUri')]"
					},
					"Snowflake1_properties_typeProperties_database": {
						"value": "[parameters('Snowflake1_properties_typeProperties_database')]"
					},
					"Snowflake1_properties_typeProperties_role": {
						"value": "[parameters('Snowflake1_properties_typeProperties_role')]"
					},
					"Snowflake1_properties_typeProperties_host": {
						"value": "[parameters('Snowflake1_properties_typeProperties_host')]"
					},
					"Snowflake2_properties_typeProperties_database": {
						"value": "[parameters('Snowflake2_properties_typeProperties_database')]"
					},
					"Snowflake2_properties_typeProperties_role": {
						"value": "[parameters('Snowflake2_properties_typeProperties_role')]"
					},
					"Snowflake2_properties_typeProperties_host": {
						"value": "[parameters('Snowflake2_properties_typeProperties_host')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "DataBiDataFactorySP25_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'DataBiDataFactorySP25_ArmTemplate_0')]"
			]
		}
	]
}